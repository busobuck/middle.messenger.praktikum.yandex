{
    "compilerOptions": {
        "outDir": "dist",
        "target": "ESNext",
        "declaration": false,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true
    }
}
